
println("OR");

if (false or false) == false {
    println("     0    0   0");
}
if (false or true) == true {
    println("     0    1   1");
}
if (true or false) == true {
    println("     1    0   1");
}
if (true or true) == true {
    println("     1    1   1");
}

println("");

println("AND");
if (false and false) == false {
    println("     0    0   0");
}
if (false and true) == false {
    println("     0    1   0");
}
if (true and false) == false {
    println("     1    0   0");
}
if (true and true) == true {
    println("     1    1   1");
}