
print "Full for loop";
for(var x = 0; x < 10; x = x + 1) {
    print x;
}

print "No declaration for loop";
var x = 0;
for(;x < 10; x = x + 1) {
    print x;
}

print "Only condition for loop";
x = 0;
for(;x < 10;) {
    print x;
    x = x + 1;
}

print "No condition for loop";
x = 0;
for(;;) {
    print x;
    x = x + 1;
}