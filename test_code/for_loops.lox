
println("Full for loop");
for(var x = 0; x < 10; x = x + 1) {
    println(x);
}

println("No declaration for loop");
var x = 0;
for(;x < 10; x = x + 1) {
    println(x);
}

println("Only condition for loop");
x = 0;
for(;x < 10;) {
    println(x);
    x = x + 1;
}

println("No condition for loop");
x = 0;
for(;;) {
    print x;
    x = x + 1;
}